            <Label Content="Enter your AGAB" FontFamily="Arial" FontWeight="Bold" HorizontalAlignment="Center" Foreground="White" Background="{x:Null}" BorderBrush="#FF040000"/>
            <ComboBox x:Name="comboAgab">
                <ComboBoxItem Content="Male"></ComboBoxItem>
                <ComboBoxItem Content="Female"></ComboBoxItem>
                <ComboBoxItem Content="Intersex"></ComboBoxItem>
            </ComboBox>
            <Label Content=""/>
            <Label Content="Enter Your Gender Identity" FontFamily="Arial" FontWeight="Bold" HorizontalAlignment="Center" Foreground="White" Background="{x:Null}" BorderThickness="0"/>
            <ComboBox x:Name="comboGender">
                <ComboBoxItem Content="Male"></ComboBoxItem>
                <ComboBoxItem Content="Female"></ComboBoxItem>
                <ComboBoxItem Content="Non-Binary"></ComboBoxItem>
                <ComboBoxItem Content="Other"></ComboBoxItem>
            </ComboBox>
            <Label Content=""/>
            <Label Content="Enter Your Romantic Orientation" FontFamily="Arial" FontWeight="Bold" HorizontalAlignment="Center" Foreground="White" Background="{x:Null}" BorderBrush="#FF040000"/>
